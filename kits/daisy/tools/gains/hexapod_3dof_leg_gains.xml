<?xml version="1.0"?>
<!--These are gains for a single 3-DoF leg of the heXapod   -->
<!--Gains are the same for each leg, and the script that    -->
<!--sets the gains on the robot loops thru each leg setting -->
<!--these gains.                                            -->
<!--                                                        -->
<!--These gains assume that the base, shoulder, elbow are   -->
<!--respectivly, an X5-4, X5-9, X5-4.                       -->
<!--                                                        -->
<!--Dave Rollinson                                          -->
<!--Oct 2017                                                -->

<group_gains>
	<control_strategies>4 4 4</control_strategies>
	<position>
		<kp>7.500000 15.000000 7.5000000</kp>
		<ki>0.000000 0.000000 0.000000</ki>
		<kd>0.000000 0.000000 0.000000</kd>
        <feed_forward>0.000000 0.000000 0.000000</feed_forward>
		<dead_zone>0.000000 0.000000 0.000000</dead_zone>
		<i_clamp>1.000000 1.000000 1.000000</i_clamp>
		<punch>0.000000 0.000000 0.000000</punch>
		<min_target>-50.265484 -50.265484 -50.265484</min_target>
		<max_target>50.265484 50.265484 50.265484 </max_target>
		<target_lowpass>1.000000 1.000000 1.000000</target_lowpass>
		<min_output>-10.000000 -20.000000 -10.000000</min_output>
		<max_output>10.000000 20.000000 10.000000</max_output>
		<output_lowpass>1.000000 1.000000 1.000000</output_lowpass>
		<d_on_error>1 1 1</d_on_error>
	</position>
	<velocity>
		<kp>0.050000 0.100000 0.050000</kp>
		<ki>0.000000 0.000000 0.000000</ki>
		<kd>0.000000 0.000000 0.000000</kd>
		<feed_forward>1.000000 1.000000 1.000000</feed_forward>
		<dead_zone>0.000000 0.000000 0.000000</dead_zone>
		<i_clamp>0.250000 0.250000 0.250000</i_clamp>
		<punch>0.000000 0.000000 0.000000</punch>
		<min_target>-3.434687 -1.502675 -3.434687</min_target>
		<max_target>3.434687 1.502675 3.434687</max_target>
		<target_lowpass>1.000000 1.000000 1.000000</target_lowpass>
		<min_output>-1.000000 -1.000000 -1.000000</min_output>
		<max_output>1.000000 1.000000 1.000000</max_output>
		<output_lowpass>0.750000 0.750000 0.750000</output_lowpass>
		<d_on_error>1 1 1</d_on_error>
	</velocity>
	<effort>
		<kp>0.250000 0.250000 0.250000</kp>
		<ki>0.000000 0.000000 0.000000</ki>
		<kd>0.000500 0.000000 0.500000</kd>
		<feed_forward>1.000000 1.000000 1.000000</feed_forward>
		<dead_zone>0.000000 0.000000 0.000000</dead_zone>
		<i_clamp>0.250000 0.250000 0.250000</i_clamp>
		<punch>0.000000 0.000000 0.000000</punch>
		<min_target>-10.000000 -20.000000 -10.000000</min_target>
		<max_target>10.000000 20.000000 10.000000</max_target>
		<target_lowpass>1.000000 1.000000 1.000000</target_lowpass>
		<min_output>-1.000000 -1.000000 -1.000000</min_output>
		<max_output>1.000000 1.000000 1.000000</max_output>
		<output_lowpass>0.500000 0.500000 0.500000</output_lowpass>
		<d_on_error>0 0 0</d_on_error>
	</effort>
</group_gains>
